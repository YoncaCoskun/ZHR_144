<core:FragmentDefinition xmlns:layout="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<Page id="wizardReviewPage3" showHeader="false">
		<content>
			<form:SimpleForm title="1. Genel Bilgiler" minWidth="1024" editable="false" layout="ResponsiveGridLayout">
				<form:content>
					<core:Title/>
					<Label text="Fiş Konusu"/>
					<Input class="inputForm" value="" enabled="false" id="fisKonuRew3"/>
					<Label text="Sicil No"/>
					<Input class="inputForm" value="" enabled="false" id="sicilNoRew3"/>
					<Label text="Ad Soyad"/>
					<Input class="inputForm" value="" enabled="false" id="adSoyadRew3"/>
					<core:Title/>
					<Label text="TC Kimlik No"/>
					<Input class="inputForm" value="" enabled="false"  id="tcRew3"/>
					<Label text="Doğum Tarihi"/>
					<DatePicker class="inputForm" value="" enabled="false"  id="dogumTarihRew3"/>
					<Label text="Geçerlilik Tarihi"/>
					<DatePicker class="inputForm" value="" enabled="false" id="gecerTarihRew3"/>
				</form:content>
			</form:SimpleForm>
			<form:SimpleForm title="2. Yeni" minWidth="1024" editable="false" layout="ResponsiveGridLayout">
				<form:content>
					<core:Title text="Mevcut"/>
					<Label text="Pozisyon"/>
					<Input class="inputForm" value="" enabled="false" id="PosAdRew3"/>
					<Label text="Personel Alanı"/>
					<Input class="inputForm" value="" enabled="false" id="perAlanRew3"/>
					<Label text="Personel Alt Alanı"/>
					<Input class="inputForm" value="" enabled="false" id="perAltAlanRew3"/>
					<Label text="İş Alanı"/>
					<Input class="inputForm" value="" enabled="false" id="isAlanRew3" />
					<Label text="Şirket"/>
					<Input class="inputForm" value="" enabled="false" id="sirketRew3"/>
					<Label text="İş Anahtarı"/>
					<Input class="inputForm" value="" enabled="false" id="isAnahRew3"/>
					<Label text="Org. Birimi"/>
					<Input class="inputForm" value="" enabled="false" id="orgBirRew3"/>
					<Label text="Çalışan Grb."/>
					<Input class="inputForm" value="" enabled="false" id="clsRew3"/>
					<Label text="Çalışan Alt Grb."/>
					<Input class="inputForm" value="" enabled="false" id="clsAltRew3"/>
					<Label text="Skala Kodu"/>
					<Input class="inputForm" value="" enabled="false" id="skalaRew3"/>
					<Label text="Ücret"/>
					<Input class="inputForm" value="" enabled="false" id="ucretRew3"/>
					<Label text="Dil Primi"/>
					<Input class="inputForm" value="" enabled="false" id="dilPrimRew3"/>
					<Label text="Araç Primi"/>
					<Input class="inputForm" value="" enabled="false" id="aracPrimRew3"/>
					<Label text="Mevsimlik Prim"/>
					<Input class="inputForm" value="" enabled="false" id="mevPrimRew3"/>
					<Label text="Vekalet Primi"/>
					<Input class="inputForm" value="" enabled="false" id="vekPrimRew3"/>
					<Label text="Diğer Bilgiler"/>
					<Input class="inputForm" value="" enabled="false" id="digerRew3"/>
					<core:Title text="Yeni"/>
					<Label text="Pozisyon"/>
					<Input class="inputForm" value="" enabled="false" id="inputPosAdRew3"/>
					<Label text="Personel Alanı"/>
					<Input class="inputForm" value="" enabled="false" id="inputPerAlanRew3"/>
					<Label text="Personel Alt Alanı"/>
					<Input class="inputForm" value="" enabled="false" id="inputPerAltAlanRew3"/>
					<Label text="Şirket"/>
					<Input class="inputForm" value="" enabled="false" id="inputsirketRew3"/>
					<Label text="İş Alanı"/>
					<Input class="inputForm" value="" enabled="false" id="inputIsAlanRew3"/>
					<Label text="İş Anahtarı"/>
					<Input class="inputForm" value="" enabled="false"  id="inputIsAnahRew3"/>
					<Label text="Org. Birimi"/>
					<Input class="inputForm" value="" enabled="false" id="inputOrgBirRew3"/>
					<Label text="Çalışan Grb."/>
					<Input class="inputForm" value="" enabled="false" id="inputClsRew3"/>
					<Label text="Çalışan Alt Grb."/>
					<Input class="inputForm" value="" enabled="false" id="inputClsAltRew3"/>
					<Label text="Skala Kodu"/>
					<Input class="inputForm" value="" enabled="false" id="inputSkalaRew3"/>
					<Label text="Ücret"/>
					<Input class="inputForm" value="" enabled="false" id="inputUcretRew3"/>
					<Label text="Dil Primi"/>
					<Input class="inputForm" value="" enabled="false" id="inputDilPrimRew3"/>
					<Label text="Araç Primi"/>
					<Input class="inputForm" value="" enabled="false" id="inputAracPrimRew3"/>
					<Label text="Mevsimlik Prim"/>
					<Input class="inputForm" value="" enabled="false" id="inputMevPrimRew3"/>
					<Label text="Vekalet Primi"/>
					<Input class="inputForm" value="" enabled="false" id="inputVekPrimRew3"/>
					<Label text="Diğer Bilgiler"/>
					<Input class="inputForm" value="" enabled="false" id="inputDigerRew3"/>
				</form:content>
			</form:SimpleForm>
			<form:SimpleForm title="3. Eğitim Bilgileri" minWidth="1024" editable="false" layout="ResponsiveGridLayout">
				<form:content>
					<core:Title/>
					<Label text="Okul Türü"/>
					<Input class="inputForm" value="Üniversite" enabled="false" id="okulRew3"/>
					<Label text="Okul Adı"/>
					<Input class="inputForm" value="Beykoz Üniversitesi" enabled="false" id="okulAdRew3"/>
					<Label text="Eğitim Dalı"/>
					<Input class="inputForm" value="Bilgisayar Mühendisliği" enabled="false" id="egitimRew3"/>
					<core:Title/>
				</form:content>
			</form:SimpleForm>
			<form:SimpleForm title="4. Yabancı Dil" minWidth="1024" editable="false" layout="ResponsiveGridLayout">
				<form:content>
					<core:Title/>
					<Table id="idLang" growing="true" growingThreshold="10" inset="false" items="{ path: '/results', sorter: { path: 'Sptxt' } }">
							<columns>
							<Column width="12em">
								<Text text="Dil Türü"/>
							</Column>
							<Column width="12em">
								<Text text="Yabancı Dil"/>
							</Column>
							<Column width="12em">
								<Text text="Puan"/>
							</Column>
							<Column width="12em">
								<Text text="Sınav Tarihi"/>
							</Column>
							<Column width="12em">
								<Text text="Yazma"/>
							</Column>
							<Column width="12em">
								<Text text="Okuma"/>
							</Column>
							<Column width="12em">
								<Text text="Dinleme"/>
							</Column>
							<Column width="12em">
								<Text text="Genel"/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Text text="{Spras}"/>
									<Text text="{Sptxt}"/>
									<Text text="{Puan}"/>
									<Text text="{SinavTarihi}"/>
									<Text text="{Yazma}"/>
									<Text text="{Okuma}"/>
									<Text text="{Dinleme}"/>
									<Text text="{Genel}"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</form:content>
			</form:SimpleForm>
			<form:SimpleForm title="5. Zihinsel Beceri" minWidth="1024" editable="false" layout="ResponsiveGridLayout">
				<form:content>
					<core:Title/>
					<Table id="idExamZihin" growing="true" growingThreshold="10" width="100%" inset="false"
						items="{ path: '/results', sorter: { path: 'SinavTuru' } }">
							<columns>
							<Column width="12em">
								<Text text="Sınav Türü"/>
							</Column>
							<Column width="12em">
								<Text text="Puan"/>
							</Column>
							<Column width="12em">
								<Text text="Sınav Tarihi"/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Text text="{SinavTuru}"/>
									<Text text="{Puan}"/>
									<Text text="{SinavTarihi}"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</form:content>
			</form:SimpleForm>
			<form:SimpleForm title="6. PD Sonuçları" minWidth="1024" editable="false" layout="ResponsiveGridLayout">
				<form:content>
					<core:Title/>
					<Table id="idPd"  growing="true" growingThreshold="10" width="100%" inset="false"
						items="{ path: '/results', sorter: { path: 'Pdyear' } }">
						<columns>
							<Column width="12em">
								<Text text="Yıl"/>
							</Column>
							<Column width="12em">
								<Text text="Sonuç"/>
							</Column>
							<Column width="12em">
								<Text text="Puan"/>
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Text text="{path: 'Pdyear' , formatter: '.vDate'}" id="yilPdRew3"/>
									<Text text="{Pdsonuc}" id="puanPdRew3"/>
									<Text text="{Pdpuan}" id="tavanPdRew3"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</form:content>
			</form:SimpleForm>
		</content>
		<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button type="Transparent" text="Geri" press="backOnayPage">
					<layoutData><OverflowToolbarLayoutData moveToOverflow="false"/></layoutData>
				</Button>
				<Button type="Emphasized" text="Devam" press="nextOnayPage">
					<layoutData><OverflowToolbarLayoutData moveToOverflow="false"/></layoutData>
				</Button>
			</OverflowToolbar>
		</footer>
	</Page>
</core:FragmentDefinition>